{
  "name": "qamon-app",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "push-patch": "git checkout development && npm version patch && git push --all && git push --tags",
    "push-minor": "git checkout development && npm version minor && git push --all && git push --tags",
    "push-major": "git checkout development && npm version major && git push --all && git push --tags",
    "test": "jest --watch",
    "extract:i18n": "formatjs extract {common,components,pages,storeZ,connectors}/**/*.{js,ts,tsx} --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "dependencies": {
    "@sentry/react": "^7.12.0",
    "@sentry/tracing": "^7.12.0",
    "@ylide/ethereum": "^0.5.2",
    "@ylide/everscale": "0.5.6",
    "@ylide/sdk": "0.5.2",
    "@ylide/smart-buffer": "0.0.9",
    "ahooks": "^3.7.0",
    "country-flag-icons": "^1.5.5",
    "date-fns": "^2.29.2",
    "everscale-inpage-provider": "^0.3.42",
    "formik": "^2.2.9",
    "linkify-react": "^4.0.0",
    "linkifyjs": "^4.0.0",
    "lodash": "^4.17.21",
    "lottie-react": "^2.3.1",
    "next": "12.2.5",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.0",
    "nprogress": "^0.2.0",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.0.5",
    "react-mitt": "^1.0.1",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.4.2",
    "yup": "^0.32.11",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.0.7",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.7.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.5",
    "eslint-plugin-formatjs": "^4.1.0",
    "jest": "^29.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  }
}
